{
  "name": "ai-assert-schema",
  "version": "1.3.0",
  "description": "Assert schemas for compatibility against your AI models",
  "packageManager": "pnpm@10.18.2",
  "types": "./dist/index.d.mts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./constraints/anthropic": "./dist/constraints/anthropic/index.mjs",
    "./constraints/google": "./dist/constraints/google/index.mjs",
    "./constraints/openai": "./dist/constraints/openai/index.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsdown",
    "test": "vitest",
    "lint": "biome check . --write",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "schema",
    "validation",
    "zod",
    "json-schema",
    "openai",
    "structured-output",
    "standard-schema"
  ],
  "author": "Chris Cook",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zirkelc/ai-assert-schema.git"
  },
  "bugs": {
    "url": "https://github.com/zirkelc/ai-assert-schema/issues"
  },
  "homepage": "https://github.com/zirkelc/ai-assert-schema#readme",
  "devDependencies": {
    "@ai-sdk/openai": "^3.0.7",
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.11",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^25.0.3",
    "ai": "^6.0.24",
    "chalk": "^5.6.2",
    "husky": "^9.1.7",
    "pkg-pr-new": "^0.0.62",
    "publint": "^0.3.16",
    "tsdown": "0.19.0-beta.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0"
  }
}
